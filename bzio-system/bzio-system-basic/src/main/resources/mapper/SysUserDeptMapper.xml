<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.bzio.system.mapper.SysUserDeptMapper">

    <resultMap type="org.bzio.system.entity.SysUserDept" id="SysUserMap">
        <result property="id" column="id" jdbcType="VARCHAR"/>
        <result property="userId" column="user_id" jdbcType="VARCHAR"/>
        <result property="deptId" column="role_id" jdbcType="VARCHAR"/>
    </resultMap>

    <insert id="insert" keyProperty="id" useGeneratedKeys="true">
        insert into sys_user_dept(id, user_id, dept_id)
        values (#{id}, #{userId}, #{deptId})
    </insert>
</mapper>

